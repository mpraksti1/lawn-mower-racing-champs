{
  "extends": "airbnb",
  "env": {
    "jest": true,
    "browser": true
  },
  "rules": {    
    "react/jsx-one-expression-per-line": "off",
    "jsx-a11y/label-has-for": "off"
  },
  "globals": {
    "cy": false,
    "React": false,
    "shallow": false,
    "render": false,
    "shallowWithTheme": false,
    "renderWithTheme": false,
    "mountWithTheme": false,
    "mount": false,
    "sinon": false,
    "fakeHistory": false,
    "theme": false,
    "localStorage": false
  }
}